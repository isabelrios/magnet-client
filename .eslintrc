{
  "plugins": [
    "react"
  ],

  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],

  "parserOptions": {
    sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },

  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },

  "rules": {
    "comma-dangle": ["always-multiline"]
  }
}